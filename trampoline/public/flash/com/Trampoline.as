package com {	import flash.display.MovieClip;		public class Trampoline extends MovieClip {				public var bounds:Object = {};		public function Trampoline() {			bounds.left = this.x - this.width/2;			bounds.right = this.x + this.width/2;			bounds.bottom = this.y;			bounds.top = this.y - this.height;						trace( bounds.left, bounds.right, bounds.bottom, bounds.top );		}	}	}